{
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"type": "AdaptiveCard",
	"version": "2.0",
	"data": {
		"disney": [
			{
				"name": "Mickey",
				"animal": "Mouse"
			},
			{
				"name": "Donald",
				"animal": "Duck"
			},
			{
				"name": "Pluto",
				"animal": "Dog"
			}
		]
	},
	"body": [
		{
			"{{#each disney}}": {
				"type": "TextBlock",
				"text": "{{name}} is a {{animal}}"
			}
		},
		{
			"type": "TextBlock",
			"text": "I am a human"
		}
	]
}
